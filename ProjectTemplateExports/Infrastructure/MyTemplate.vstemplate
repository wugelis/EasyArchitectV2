  <TemplateContent>
    <Project TargetFileName="Infrastructure.csproj" File="Infrastructure.csproj" ReplaceParameters="true">
      <Folder Name="Persistance" TargetFolderName="Persistance">
        <Folder Name="Configurations" TargetFolderName="Configurations" />
      </Folder>
